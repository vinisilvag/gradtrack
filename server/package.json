{
  "name": "gradtrack-server",
  "version": "0.1.0",
  "main": "src/server.ts",
  "repository": "https://github.com/vinisilvag/gradtrack",
  "license": "MIT",
  "engines": {
    "node": "22"
  },
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "tsx -r tsconfig-paths/register src/server.ts",
    "start:prod": "node dist/src/server.js",
    "start:dev": "tsx watch -r tsconfig-paths/register src/server.ts",
    "seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@biomejs/biome": "2.3.3",
    "@faker-js/faker": "^10.1.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "module-alias": "^2.2.3",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
